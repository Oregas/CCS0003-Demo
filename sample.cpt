//examples of data types 
#include <iostream>
using namespace std;

int main () 

{		int number=20; //integer 
	const double pi = 3.151659; //double
	char letter = 'A';
	bool isValid = true; 
	cout << "Value of number:" << number <<endl;
	cout << "Value of PI" << pi << endl;
	cout << "Value of Letter" << letter << endl;
	cout << "Value of isValid" << (isValid ? "True": "False") <<endl;
	return 0;
}
